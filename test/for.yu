import 'queue.yu'

for j=1,3 do
	print('>>>> cycle',j)
	if j==2 then
		print('skip cycle 2')
		continue
	end
	for i=1,5 do
		if i==3 then
			print('skip 3')
			continue
		end
		if i%2==0 then
			print('even',i)
		else
			print('odd',i)
		end
	end
end

for i,color in {'red','yellow','blue'} do
	print(i,color)
end

for lower,upper in {a='A',b='B',c='C'} do
	print(lower,upper)
end

