import 'mulval.yu'
extern
	func print(...*)
end

class Vec
	field x,y,z#
	method __new(nx,ny,nz#)
		x,y,z=nx,ny,nz
	end
	
	method lenth()=>x^2+y^2+z^2
end

class Sprite
	field pos=Vec(0,0,0)
	field size#
	method update()
	end
	method draw()
		print('drawing')
	end
	method __new()
		print('new sprite')
	end
end

class ImageSprite extends Sprite
	field img='sprite.png'
	method setImage(img$)
		self.img=img
	end

	method register()
		print('register image sprite:',img)
	end

	method draw()
		super.draw()
		print('imagesprite')
	end

	method __new()
		img='test'
		register()
	end
end
-- Sprite()
local i=ImageSprite()
i.draw()

ImageSprite{size=20}

func uploadSprite(sprite:Sprite)
end

uploadSprite({
		size=20
	})